CREATE TABLE ESM_ROLE (ROLE_NAME VARCHAR(255) NOT NULL, DELETE_USER_PRIV VARCHAR(255), IMPORT_PRIV VARCHAR(255), UPDATE_USER_PRIV VARCHAR(255), USER_ID VARCHAR(255), PRIMARY KEY (ROLE_NAME))
CREATE TABLE ESM_USER (USER_ID VARCHAR(255) NOT NULL, ADDRESS1 VARCHAR(255), ADDRESS2 VARCHAR(255), CITY VARCHAR(255), COUNTRY VARCHAR(255), EMAIL_ID VARCHAR(255), MOBILE_NO VARCHAR(255), PASSWORD VARCHAR(255), PINCODE VARCHAR(255), STATE VARCHAR(255), PRIMARY KEY (USER_ID))
CREATE TABLE ESM_USER_ROLE (USER_ID VARCHAR(255) NOT NULL, ROLE_NAME VARCHAR(255) NOT NULL, PRIMARY KEY (USER_ID, ROLE_NAME))
ALTER TABLE ESM_ROLE ADD CONSTRAINT FK_ESM_ROLE_USER_ID FOREIGN KEY (USER_ID) REFERENCES ESM_USER (USER_ID)
ALTER TABLE ESM_USER_ROLE ADD CONSTRAINT FK_ESM_USER_ROLE_USER_ID FOREIGN KEY (USER_ID) REFERENCES ESM_USER (USER_ID)
ALTER TABLE ESM_USER_ROLE ADD CONSTRAINT FK_ESM_USER_ROLE_ROLE_NAME FOREIGN KEY (ROLE_NAME) REFERENCES ESM_ROLE (ROLE_NAME)
